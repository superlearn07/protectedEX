{
  "manifest_version": 3,
  "name": "protectedEX",
  "version": "1.0.0",
  "description": "It stores your data/ notes securely without any intervention",
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage"
  ],
  "host_permissions": [
  "https://*/*"
],

  "background": {
    "service_worker": "background.js"
  }
}
